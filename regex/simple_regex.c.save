#include <stdio.h>
#include <regex.h>

int main(void) {
	regex_t reg;

	char pattern[] = "s?ck";

	regmatch_t offsets[3];

	int res = regcomp(&reg, pattern, REG_ICASE | REG_NEWLINE);

	if(res) {
		char erbuf[256];
		regerror(res, &reg, erbuf, sizeof(erbuf));

		if(res < 1)
			printf("Cannot get the error string\n");
	}
//	printf("%d\n", res);

	
	regfree(&reg);
	 return 0;
}
